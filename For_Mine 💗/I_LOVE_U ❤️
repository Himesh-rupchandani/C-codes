#include<iostream>
using namespace std;

void space(){
    for(int j=0;j<2;j++){
        cout << " ";
    }
}

int main(){
    // I-Part
    for(int i=0;i<6;i++){
        for(int j=0;j<3;j++){
            cout << "*";
        }
        space();
        space();
        space();
        
        // L-Part
        for(int j=0;j<3;j++){
            cout << "*";
        }
        for(int j=0;j<5;j++){
            if(i>=0 && i<=3) cout << " ";
            if((i==4 || i==5) && j<=4){
                cout << "*";
            }
        }
        space();
        
        // O-Part
        for(int j=0;j<8;j++){
            if(((i==0 || j==0) || (i==1 || j==1)) || (i==5 || j==7) || (i==4 || j==6)) cout << "*";
            else cout <<" ";
        }
        space();
        
        // V-Part
        for(int j=0; j<13; j++) {
        if (j == i || j == i + 1 || j == 12 - i || j == 11 - i) cout << "*";
        else if (j>i+1 && j<11-i) cout << " ";
        else cout << " ";
    }
        space();
        
        // E-Part
           for(int j=0;j<2;j++){
               if(i==1 || i==4) cout << "*";
           }
           for(int j=0;j<9;j++){
               if(i==0 || i==5) cout << "*";
           }
           for(int j=0;j<5;j++){
               if(i==2 || i==3) cout << "*";
           }
           space();
           space();
           space();
           

            cout << endl;
    }
    return 0;
}

